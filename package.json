{
  "name": "rs-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --color",
    "lint:styles": "stylelint 'src/**/*.scss' --fix",
    "format": "prettier . --check && npx stylelint ./src/**/*.scss",
    "format:fix": "prettier . --write  && npx stylelint ./src/**/*.scss --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest",
    "test:memory": "node --max-old-space-size=2048 node_modules/vitest/vitest.mjs",
    "test:debug": "node --inspect-brk node_modules/vitest/vitest.mjs",
    "test:profile": "node --inspect node_modules/vitest/vitest.mjs",
    "test:coverage": "node --max-old-space-size=2048 ./node_modules/vitest/vitest.mjs run --coverage"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.84.1",
    "file-saver": "^2.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "clsx": "^2.1.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "eslint-plugin-react-dom": "^1.52.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.52.2",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.2",
    "sass": "^1.89.2",
    "stylelint": "^16.21.1",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
